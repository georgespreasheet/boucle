function updateSheetFromGoogleSheet() {
  // Récupère la feuille de calcul de destination active
  var sheet = SpreadsheetApp.getActiveSheet();
  
  // Récupère les données de la feuille de calcul de destination sous forme de tableau
  var data = sheet.getDataRange().getValues();
  
  // Récupère la feuille de calcul source
  var sourceId = 'ID_DE_LA_FEUILLE_DE_CALCUL_SOURCE';
  var source = SpreadsheetApp.openById(sourceId);
  
  // Récupère les données de la feuille de calcul source sous forme de tableau
  var sourceData = source.getActiveSheet().getDataRange().getValues();
  
  // Crée un tableau pour stocker les données à ajouter à la feuille de calcul de destination
  var newData = [];
  
  // Boucle à travers chaque ligne des données de la feuille de calcul source
  for (var i = 0; i < sourceData.length; i++) {
    // Récupère la ligne actuelle de la feuille de calcul source
    var row = sourceData[i];
    
    // Initialise une variable pour suivre si la ligne existe déjà dans la feuille de calcul de destination
    var rowExists = false;
    
    // Compare les données de la feuille de calcul source à celles de la feuille de calcul de destination pour trouver une correspondance
    for (var j = 0; j < data.length; j++) {
      // Récupère la ligne actuelle de la feuille de calcul de destination
      var sheetRow = data[j];
      
      // Compare la première colonne des deux lignes
      if (row[0] == sheetRow[0]) {
        // Met à jour la variable pour indiquer que la ligne existe déjà
        rowExists = true;
        
        // Sort de la boucle de comparaison
        break;
      }
    }
    
    // Si la ligne n'existe pas déjà dans la feuille de calcul de destination, l'ajoute aux données à ajouter
    if (!rowExists) {
      newData.push(row);
    }
  }
  
  // Si des nouvelles données ont été trouvées, les ajoute à la feuille de calcul de destination
  if (newData.length > 0) {
    sheet.getRange(data.length+1, 1, newData.length, newData[0].length).setValues(newData);
  }
}
