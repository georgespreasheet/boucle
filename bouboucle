function removeRows() {
  // Récupère les feuilles source et destination
  const sourceSheet = SpreadsheetApp.getActiveSpreadsheet().getSheetByName('Feuille source');
  const destinationSheet = SpreadsheetApp.getActiveSpreadsheet().getSheetByName('Feuille destination');
  
  // Récupère les données des feuilles source et destination
  const sourceData = sourceSheet.getRange('B2:B').getValues().flat();
  const destinationData = destinationSheet.getRange('B2:B').getValues().flat();
  
  // Stocke les index des lignes à supprimer
  const indexesToDelete = destinationData.reduce((indexes, row, index) => {
    if (!sourceData.includes(row)) {
      indexes.push(index + 2);
    }
    return indexes;
  }, []);
  
  // Supprime les lignes de la feuille destination
  indexesToDelete.reverse().forEach(index => {
    destinationSheet.deleteRow(index);
  });
}
